# Datalink Analytics Dashboard Environment Variables
# Copy this file to .env and fill in your values

# ===================
# Google OAuth2
# ===================
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-your-google-client-secret
GOOGLE_API_KEY=your-google-api-key

# ===================
# Google Analytics
# ===================
GA_MEASUREMENT_ID=G-XXXXXXXXXX
GA_STREAM_ID=0000000000
GA_PROPERTY_ID=0000000000

# ===================
# Google Ads
# ===================
GOOGLE_ADS_DEVELOPER_TOKEN=your-developer-token
GOOGLE_ADS_CUSTOMER_ID=1234567890

# ===================
# HubSpot
# ===================
HUBSPOT_PRIVATE_APP_TOKEN=pat-na1-xxxxxxxx
HUBSPOT_CLIENT_SECRET=xxxxxxxx

# ===================
# Meta Ads
# ===================
META_APP_ID=your-meta-app-id
META_APP_SECRET=your-meta-app-secret
META_CLIENT_ID=your-meta-client-id
META_AD_ACCOUNT_ID=your-ad-account-id

# ===================
# API URL (for frontend)
# ===================
VITE_API_URL=http://localhost:3001
